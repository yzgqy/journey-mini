<template name="journeyItem">
  <view class="journey-container">
    <text class="journey-title">{{theme}}</text>
    <text class="journey-content">{{desc}}
            </text>
    <view class="journey-author-date">
      <text class="journey-date">开始时间：{{startTime}}</text>
      <text class="journey-date">结束时间：{{endTime}}</text>
    </view>
  </view>
</template>