<view class="page-body">
  <view class="page-section page-section-gap">
    <map
      id="myMap"
      style="width: 100%; height: 400px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      show-location
    ></map>
  </view>
  <view class="btn-area">
    <button bindtap="chooseLocation" class="page-body-button" type="primary">位置有偏差</button>
  </view>
</view>